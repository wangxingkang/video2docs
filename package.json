{
  "name": "video2docs",
  "version": "1.0.1",
  "description": "将视频截图并转换为文档",
  "private": true,
  "main": "main/index.cjs.js",
  "scripts": {
    "start": "node ./scripts/watch.js",
    "build": "npm run package:all",
    "build:src": "cross-env MODE=production walrus run ./scripts/build.ts",
    "build:icon": "electron-icon-builder --input=./assets/logo.png --output=./build",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir src",
    "package:mac": "npm run build:src && electron-builder build --mac -p never",
    "package:win": "npm run build:src && electron-builder build --win -p never",
    "package:linux": "npm run build:src && electron-builder build --linux -p never",
    "package:all": "npm run build:src && electron-builder build -mwl -p never",
    "release": "walrus release --skip-publish --skip-build"
  },
  "dependencies": {
    "@ffmpeg-installer/ffmpeg": "1.1.0",
    "@ffprobe-installer/ffprobe": "1.2.0",
    "@walrus/images-pptx": "1.1.0",
    "antd": "4.16.13",
    "electron-updater": "4.3.9",
    "fluent-ffmpeg": "^2.1.2",
    "imagemin": "7.0.1",
    "imagemin-pngquant": "8.0.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-router-dom": "5.3.0"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "2.2.0",
    "@types/fluent-ffmpeg": "^2",
    "@types/imagemin": "^7",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/react-router-dom": "^5",
    "@types/rimraf": "^3",
    "@vitejs/plugin-react-refresh": "1.3.6",
    "@walrus/cli": "^1.3.4",
    "@walrus/plugin-release": "^1.14.0",
    "@walrus/plugin-run": "^0.5.0",
    "cross-env": "7.0.3",
    "electron": "15.3.0",
    "electron-builder": "22.13.1",
    "electron-devtools-installer": "3.2.0",
    "electron-icon-builder": "^2.0.1",
    "electron-rebuild": "^3.2.3",
    "electron-releases": "3.843.0",
    "less": "^4.1.2",
    "less-vars-to-js": "^1.3.0",
    "rimraf": "^3.0.2",
    "spectron": "15.0.0",
    "typescript": "4.4.4",
    "vite": "2.6.13",
    "vite-plugin-imp": "^2.0.9"
  },
  "packageManager": "yarn@3.0.1",
  "engines": {
    "node": ">=14.16",
    "npm": ">=7.7"
  }
}
